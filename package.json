{
  "name": "remote-gcode-sender",
  "version": "0.1.0",
  "description": "This is a prototype for a remote uart gcode sender.",
  "main": "dist/server/index.js",
  "private": true,
  "scripts": {
    "start": "npm run runServer",
    "build": "npm run buildApp && npm run buildServer",
    "server": "npm run buildServer && npm run runServer",
    "runServer": "echo \"Running: Server\" && node .",
    "buildServer": "echo \"Building: Server\" && tsc --project src/server/tsconfig.json",
    "buildApp": "echo \"Building: App\" && cross-env TS_NODE_PROJECT=\"src/app/tsconfig.json\" webpack",
    "dev": "webpack --watch & npm run devServer & nodemon . --watch dist/server",
    "devApp": "webpack serve --env isDevServer",
    "devServer": " tsc --project src/server/tsconfig.json -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gabriel G. Martins",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@types/d3": "^5.16.3",
    "@types/express": "^4.17.8",
    "@types/multer": "^1.4.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/serialport": "^8.0.1",
    "@types/webpack": "^4.41.25",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "d3": "^6.2.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.6.3",
    "serialport": "^9.0.2",
    "ws": "^7.3.1"
  }
}
