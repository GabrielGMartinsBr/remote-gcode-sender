{
  "name": "remote-gcode-sender",
  "version": "0.1.0",
  "description": "This is a prototype for a remote uart gcode sender.",
  "main": "dist/server/index.js",
  "private": true,
  "scripts": {
    "start": "npm run runServer",
    "build": "npm run buildApp && npm run buildServer",
    "server": "npm run buildServer && npm run runServer",
    "runServer": "echo \"Running: Server\" && node .",
    "buildServer": "echo \"Building: Server\" && tsc --project tsconfig.server.json",
    "buildApp": "echo \"Building: App\" && webpack",
    "dev": "webpack --watch & npm run devServer & nodemon . --watch dist/server",
    "devApp": "webpack serve --env isDevServer",
    "devServer": " tsc --project tsconfig.server.json -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gabriel G. Martins",
  "license": "ISC",
  "devDependencies": {
    "@types/d3": "^5.16.3",
    "@types/express": "^4.17.8",
    "@types/serialport": "^8.0.1",
    "@types/ws": "^7.2.9",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "d3": "^6.2.0",
    "express": "^4.17.1",
    "rxjs": "^6.6.3",
    "serialport": "^9.0.2",
    "ws": "^7.3.1"
  }
}
